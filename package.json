{
  "name": "familysearch-javascript-sdk",
  "version": "2.6.0",
  "description": "Javascript SDK for FamilySearch",
  "repository": {
    "type": "git",
    "url": "git://github.com/familysearch/familysearch-javascript-sdk.git"
  },
  "keywords": [
    "familysearch"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rootsdev/familysearch-javascript-sdk/issues"
  },
  "main": "src/FamilySearch.js",
  "dependencies": {
    "es6-promise": "3.0.2",
    "isomorphic-fetch": "2.1.1"
  },
  "devDependencies": {
    "browserify": "12.0.1",
    "coveralls": "2.11.2",
    "grunt": "0.4.5",
    "grunt-contrib-clean": "0.4.1",
    "grunt-contrib-copy": "0.5.0",
    "grunt-contrib-jshint": "0.7.1",
    "grunt-contrib-uglify": "0.8.0",
    "grunt-gh-pages": "0.9.1",
    "grunt-ngdocs": "0.1.11",
    "grunt-run": "0.3.0",
    "istanbul": "0.3.13",
    "jasmine": "2.2.1",
    "jshint": "2.3.0",
    "lodash": "3.1.0",
    "mitm": "1.1.0"
  },
  "scripts": {
    "travis": "grunt travis --verbose",
    "test": "grunt test --verbose",
    "build": "grunt build",
    "jasmine": "jasmine",
    "browserify": "browserify src/FamilySearch.js --standalone FamilySearch -i ./src/modules/node-only.js -o dist/familysearch-javascript-sdk.js",
    "coverage": "istanbul cover ./node_modules/jasmine/bin/jasmine.js",
    "coveralls": "istanbul cover ./node_modules/jasmine/bin/jasmine.js && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage"
